@book{Torvalds:2001,
	author					= {Linus B. Torvalds and David Diamond},
	title						= {Il était une fois \textsc{Linux}},
	subtitle				= {L'extraordinaire histoire d'une révolution accidentelle},
	date						= {2001},
	%editor					= {},
	%editortype			= {collaborator},
